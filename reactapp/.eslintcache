oadData() {\n      const response = await fetch('/new-movie');\n      const jsonResponse = await response.json();\n \n      setMovieList(jsonResponse.movies)\n    };\n    loadData()\n\n    // Mise à jour de la Wishlist\n    const get_Wishlist = async () => {\n      let request = await fetch('/wishlist');\n      let response = await request.json();\n      setmoviesCount(response.length)\n      setmoviesWishList(response)\n    }\n    get_Wishlist()\n    // Se déclenche à l'initialisation du composant\n  }, [])\n\n\n  // Ajoute une film dans wishlist\n\n  var handleClickAddMovieParent = async (movieName, movieImg) => {\n    // Déclenche la route qui va ajouter un film à la BDD\n    let request = await fetch('/wishlist', {\n\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/x-www-form-urlencoded'\n      },\n      body: `movieName=${movieName}&movieImg=${movieImg}`\n    });\n\n    let response = await request.json();\n\n\n    setmoviesCount(moviesCount + 1);\n\n    // Ajoute un élément à notre tableau / état wishlist\n    setmoviesWishList([...moviesWishList, {\n      movieName: movieName,\n      movieImg: movieImg\n    }]);\n  }\n\n\n  // Supprime un film de la wishlist\n  var handleClickDeleteMovie = async (name) => {\n    // Déclenche la route qui va supprimer le film en BDD\n    let request = await fetch(`/wishlist/${name}`, {\n      method: 'DELETE'\n    })\n    let response = await request.json()\n\n    setmoviesCount(moviesCount - 1);\n\n    // Parcourir le tableau / Créer un nouvaeu tableau avec les éléments qui remplissent la condition (object.name != name)\n    // Condition : si le nom de l'objet est différent du nom du film ciblé (argument de la fonction) \n    setmoviesWishList(moviesWishList.filter((object) => object.movieName !== name))\n  }\n\n  // Créer un nouveau tableau à partir des éléments de movieswishlist en les mettant en forme (les créer)\n  var movies = movieList.map((movies, i) => {\n\n      // À la création de mon composant recherche sur le nom de l'élément apparaît dans la wishlist\n      // Parcourir le tableau /  stocker dans result les éléments qui remplissent la condition (element.name == movie.title)\n      // Condition : si le nom de l'objet est similaire au nom du film créé\n\n     \n      var result = moviesWishList.find((e) => e.movieName === movies.title)\n\n      // Marquer un film comme \"liké\" ou pas\n      // On s'appuie sur la valur de result pour faire varier la valeur de is_liked\n      var isLiked = false\n      if (result !== undefined) {\n        isLiked = true\n      }\n\n\n      // // Pour que la description du film ne dépasse pas 80 caractères\n      // var result = movies.overview\n      // if(result.length > 80){\n      //   result = result.slice(0,80)+'...'\n      // }\n      // envoyer result à la place de movieDesc\n\n\n      return ( \n      < CardMovie key = {i}\n        handleClickAddMovieParent = {\n          handleClickAddMovieParent\n        }\n        handleClickDeleteMovie = {\n          handleClickDeleteMovie\n        }\n        movieName = {movies.title}\n        movieDesc = {movies.overview}\n        movieImg = {movies.poster_path}\n        globalRating = {movies.vote_average}\n        globalCountRating = {movies.vote_count}\n        watchedHeart = {isLiked}\n        />)\n      })\n\n    return ( \n    <div style = {{backgroundColor: 'black'}} >\n      <Container> \n     \n        {/* Envoi composant navbar */}\n      <NavBar moviesCount = {moviesCount}\n      moviesWishList = {moviesWishList}\n      handleClickDeleteMovie = {handleClickDeleteMovie}\n      movieImg = {moviesWishList.movieImg}\n      movieName = {movies.title}\n      />  \n    \n      <Row>\n        {/* réception composant */}\n      {movies}\n      </Row>\n\n      </Container>  \n      </div >\n    );\n\n  }\n\n  export default App;","/home/rachel/Dev/Capsule/Project/my_mooviz/part5/reactapp/src/reportWebVitals.js",[],"/home/rachel/Dev/Capsule/Project/my_mooviz/part5/re